---
interface Props {
  title: string
  description: string
  image: string
  keywords: string
}
---

<meta name='keywords' content={'post'} />
<!-- Primary Meta Tags -->
<meta name='title' content={'post'} />
<meta name='description' content={'post'} />
<!-- Open Graph / Facebook -->
<meta property='og:type' content='website' />
<meta property='og:url' content='https://www.rxtsel.dev' />
<meta property='og:title' content={'post'} />
<meta property='og:description' content={'post'} />
<meta
  property='og:image'
  content='https://www.rxtsel.dev/meta_tags_large_icon.png'
/>
<!-- Twitter -->
<meta property='twitter:card' content='summary_large_image' />
<meta property='twitter:url' content='https://www.rxtsel.dev' />
<meta property='twitter:title' content={'post'} />
<meta property='twitter:description' content={'post'} />
<meta
  property='twitter:image'
  content='https://www.rxtsel.dev/meta_tags_large_icon.png'
/>
